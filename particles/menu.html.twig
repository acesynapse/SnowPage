{% set one = '[wpa_toolbar]' %}
{% set two = '[google-translator]' %}
{% set url = url(particle.url)|default(gantry.siteUrl()) %}

<div class="uk-hidden@m">
<a class="uk-logo" href="{{ url }}">{{ particle.svg|raw }}</a></div>

<nav class="uk-navbar-container" uk-navbar>
    <div class="uk-navbar-left uk-visible@m">
	<a class="uk-navbar-item uk-logo" href="{{ url }}">{{ particle.svg|raw }}</a>
  <ul class="uk-navbar-nav uk-margin-left">
  {% for item in menu.root %}
    {% if not item.children %}
      <li class="uk-active">
        <a class="sp-color-1-5" href="{{ item.link }}">{{ item.title }}</a>
      </li>
    {% endif %}
    {% if item.children %}
      <li class="uk-parent">
        <a class="sp-color-1-5" href="{{ item.link }}">{{ item.title }}
          <span uk-navbar-parent-icon="uk-navbar-parent-icon"></span></a>
        <div class="uk-navbar-dropdown">
          <ul class="uk-nav uk-navbar-dropdown-nav">
            {% for child in item.children %}
              <li>
                <a class="sp-color-1-5" href="{{ child.link }}">{{ child.title }}</a>
              </li>
            {% endfor %}
          </ul>
        </div>
      </li>
    {% endif %}
  {% endfor %}
</ul>
</div>

<div class="uk-navbar-right">
<ul class="uk-iconnav">
  <li class="uk-hidden@m">
    <button class="uk-button uk-button-link sp-color-1-5" type="button" uk-toggle="target: #offcanvas-nav-primary">
      <span uk-icon="icon: table; ratio: 1.5"></span><br>Menu</button>
  </li>
  {% for item in particle.items %}
    <li>
      <a href="{{ item.link|e }}" target="{{ item.target|e }}">
        <button class="uk-button uk-button-link sp-color-1-5" type="button">
          <span uk-icon="icon: {{ item.icon|e }}; ratio: 1.5"></span><br>{{ item.text|e }}</button>
      </a>
    </li>
  {% endfor %}
  <li class="uk-inline">
    <button class="uk-button uk-button-link sp-color-1-5" type="button">
      <span uk-icon="icon: settings; ratio: 1.5"></span><br>Settings</button>
    <div uk-dropdown="mode: click">
      <div class="  ltr  left a11y-toolbar-widget">
        <ul style="float:left;list-style:none;padding-left:unset;">
          <li style="float:left;">
            <button type="button" class="a11y-toggle a11y-toggle-contrast toggle-contrast" id="is_normal_contrast" aria-pressed="false">
              <span class="aticon aticon-adjust" aria-hidden="true" title="Toggle Contrast"></span>
            </button>
          </li>
          <li style="float:left;">
            <button type="button" class="a11y-toggle a11y-toggle-fontsize toggle-fontsize" id="is_normal_fontsize" aria-pressed="false">
              <span class="aticon aticon-font" aria-hidden="true" title="Toggle Font Size"></span>
            </button>
          </li>
		  <li style="float:left;">
            <button type="button" class="a11y-toggle a11y-toggle-grayscale toggle-grayscale" id="is_normal_color" aria-pressed="false">
              <span class="aticon aticon-tint" aria-hidden="true" title="Toggle Greyscale"></span>
            </button>
          </li>
        </ul>
      </div>
      {{ gantry.platform.filter(two)|html|raw }}
    </div>
  </li>
  <li>
    <a href="https://catalog.{{ title|e }}/cgi-bin/koha/opac-user.pl" target="_blank">
      <button class="uk-button uk-button-link sp-color-1-5" type="button">
        <span uk-icon="icon: user; ratio: 1.5"></span><br>Account</button>
    </a>
  </li>
  <li>
    <a href="https://catalog.{{ title|e }}/">
      <button class="uk-button uk-button-link sp-color-1-5" type="button">
        <span uk-icon="icon: search; ratio: 1.5"></span><br>Search</button>
    </a>
  </li>
</ul>
</div>
</nav>

<div id="offcanvas-nav-primary" uk-offcanvas="overlay: true">
<div class="uk-offcanvas-bar uk-flex uk-flex-column uk-background-default">
<ul class="uk-nav uk-nav-primary uk-margin-auto-vertical" uk-nav="uk-nav">
  {% for item in menu.root %}
    {% if not item.children %}
      <li class="uk-active">
        <a class="sp-color-1-5" href="{{ item.link }}">{{ item.title }}</a>
      </li>
    {% endif %}
    {% if item.children %}
      <li class="uk-parent">
        <a class="sp-color-1-5" href="{{ item.link }}">{{ item.title }}
          <span uk-nav-parent-icon="uk-nav-parent-icon"></span></a>
        <ul class="uk-nav-sub">
          {% for child in item.children %}
            <li>
              <a class="sp-color-1-5" href="{{ child.link }}">{{ child.title }}</a>
            </li>
          {% endfor %}
        </ul>
      </li>
    {% endif %}
  {% endfor %}
</ul>
</div>
</div>
